*color14:#6d715e

!white
*color7:#c0b18b
*color15:#c0b18b

! Advanced tabs

URxvt.perl-ext-common: ...,tabbedex,...
URxvt.tabbed.autohide: true
URxvt.tabbed.reopen-on-close: yes

URxvt.keysym.Control-t: perl:tabbedex:new_tab
URxvt.keysym.Control-Right: perl:tabbedex:next_tab
URxvt.keysym.Control-Left: perl:tabbedex:prev_tab

urxvt*font: xft:xos4 Terminus:regular:pixelsize=12:antialias=true:autohint=true
urxvt*boldFont: xft:xos4 Terminus:regular:pixelsize=12:antialias=true:autohint=true
urxvt*italicFont: xft:xos4 Terminus:regular:pixelsize=12:antialias=true:autohint=true
urxvt*bolditalicFont: xft:xos4 Terminus:regular:pixelsize=12:antialias=true:autohint=true

! Stolen, not sure of all of this
urxvt*cursorUnderline: 1
urxvt*letterspace: -5
urxvt*cursorBlink:		true
urxvt*scrollBar:		false
! urxvt*depth:			32 
urxvt*internalBorder: 		5
! urxvt.urgentOnBell:  true

Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb


